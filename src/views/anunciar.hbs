  <form id="cepForm" action="/auth/cadRep" method="POST" enctype="multipart/form-data">
    <main class="containers">
      <div class="container-cont">
        <div class="titulo">
          <p>Preencha com os dados da República</p> <svg xmlns="http://www.w3.org/2000/svg" width="37" height="21"
            viewBox="0 0 37 21" fill="none">
            <path
              d="M1 19.9926C1 16.5694 5.73893 16.5694 8.1084 14.2873C9.29314 13.1462 5.73893 13.1462 5.73893 7.44092C5.73893 3.63775 7.31818 1.7356 10.4779 1.7356C13.6376 1.7356 15.2168 3.63775 15.2168 7.44092C15.2168 13.1462 11.6626 13.1462 12.8473 14.2873C15.2168 16.5694 19.9557 16.5694 19.9557 19.9926M18.3678 17.3197C19.2634 16.7251 20.4261 16.2971 21.5387 15.781C22.4086 15.3775 23.2478 14.9201 23.9049 14.2873C25.0896 13.1462 21.5354 13.1462 21.5354 7.44092C21.5354 3.63775 23.1146 1.7356 26.2743 1.7356C29.434 1.7356 31.0133 3.63775 31.0133 7.44092C31.0133 13.1462 27.4591 13.1462 28.6438 14.2873C31.0133 16.5694 35.7522 16.5694 35.7522 19.9926"
              stroke="#919191" stroke-width="1.5" stroke-linecap="round" />
          </svg>
        </div>
        <div class="cont-dados">
          <div class="dados-1">
            <p>Nome</p>
            <input class="input-dados" type="text" id="ds_nomeAnfitriao" name="ds_nomeAnfitriao" maxlength="80">
            <p>E-mail</p>
            <input class="input-dados" type="text" id="ds_emailAnfitriao" name="ds_emailAnfitriao">
          </div>
          <div class="dados-2">
            <p>Telefone ou Celular para contato</p>
            <input class="input-dados" type="tel" placeholder="(XX) XXXXX-XXXX" id="nmr_telefoneAnfitriao" name="nmr_telefoneAnfitriao" maxlength="15" oninput="mascaraTelefone(this)">                  
            <p>Ano de Criação da república:</p>
            <input class="input-dados" id="an_anoCriacao" type="text" name="an_anoCriacao" maxlength="4">
          </div>
        </div>
      </div>

      <div class="container-cont">
        <div class="titulo">
          <p>Localização</p>
          <svg xmlns="http://www.w3.org/2000/svg" width="37" height="25" viewBox="0 0 37 25" fill="none">
            <path
              d="M12.6667 1.47534L1 3.98568V24.0684L12.6667 21.5581M12.6667 1.47534V21.5581M12.6667 1.47534L24.3333 3.98568M12.6667 21.5581L24.3333 24.0684M24.3333 3.98568L36 1.47534V21.5581L24.3333 24.0684M24.3333 3.98568V24.0684"
              stroke="#909090" stroke-width="1.5" stroke-linejoin="round" />
          </svg>
        </div>

        <div class="cont-cep">
          <div class="cep">
            <p>Cep:</p>
            <input class="input-dados" type="text" id="ds_cep" name="ds_cep" required maxlength="9">
            <p>Cidade:</p>
            <input class="input-dados" type="text" id="ds_cidade" name="ds_cidade" readonly>
            <p>Estado:</p>
            <input class="input-dados" type="text" id="ds_estado" name="ds_estado" readonly>
            <p>Rua:</p>
            <input class="input-dados" type="text" id="ds_rua" name="ds_rua" readonly>
            <p>Bairro:</p>
            <input class="input-dados" type="text" id="ds_bairro" name="ds_bairro" readonly>
            <p>Numero:</p>
            <input class="input-dados" type="text" id="ds_numero" name="ds_numero">
          </div>
        </div>
      </div>

      <div class="container-cont-desc">
        <div class="titulo">
          <p>Descrição e Regras</p>
          <svg xmlns="http://www.w3.org/2000/svg" width="27" height="24" viewBox="0 0 27 24" fill="none">
            <path
              d="M10.2719 12.296L1.37654 3.44549C0.50588 2.57922 1.24185 1.25659 2.59455 1.25659H24.4055C25.7582 1.25659 26.4941 2.57922 25.6235 3.44549L16.6379 12.3857V18.6066L10.2719 22.6018V12.296Z"
              stroke="#909090" stroke-width="1.5" stroke-linejoin="round" />
          </svg>
        </div>

        <div class="cont-regras">
          <p>Tipos de República</p>
          <div>
            <select name="tipoRep" id="option">
              <option value="Mista">Mista</option>
              <option value="Fem">Feminina</option>
              <option value="Masc">Masculino</option>
            </select>
          </div>

          <p>Tipos de Imóvel</p>
          <div>
            <select name="imovel" id="option">
              <option value="Casa">Casa</option>
              <option value="Apto">Apartamento</option>
            </select>
          </div>

          <div class="buttons-quantidade">
            <div class="quartos">
              <p>Quartos</p>
              <div class="button-mes">
                <input type="text" name="qtd_quarto" id="">
              </div>
            </div>
            <div class="banheiros">
              <p>Banheiros</p>
              <div class="button-mes">
                <input type="text" name="qtd_banheiro" id="">
              </div>
            </div>
          </div>
        </div>

        <div class="regras-radio">
          <h1>Regras da República</h1>
          <div>
            <p>Permitido Fumar?</p>
            <select name="fumar" id="option">
              <option value="1">Sim</option>
              <option value="0">Nao</option>
            </select>
          </div>
          <div>
            <p>Permitido Pets?</p>
            <select name="pets" id="option">
              <option value="1">Sim</option>
              <option value="0">Nao</option>
            </select>
          </div>
          <div>
            <p>Permitido Bebidas alcoólicas?</p>
            <select name="bebidas" id="option">
              <option value="1">Sim</option>
              <option value="0">Nao</option>
            </select>
          </div>
          <div>
            <p>Permitido visitas?</p>
            <select name="visitas" id="option">
              <option value="1">Sim</option>
              <option value="0">Nao</option>
            </select>
          </div>
        </div>

        <div class="comodidades">
          <h1>Comodidades</h1>
          
          
          <div>
            <input value="1" type="checkbox" style="opacity: 0;"name="wifi" id="wifi">
            <label for="wifi"><img src="/wi-fi.png"></label>

            <input value="1" type="checkbox" style="opacity: 0;" name="tv" id="tv">
            <label for="tv"><img src="/tv.png"></label>

            <input value="1" type="checkbox" style="opacity: 0;" name="cozinha" id="cozinha">
            <label for="cozinha"><img src="/cozinhau.png"></label>

            <input value="1" type="checkbox" style="opacity: 0;" name="estacionamento" id="estacionamento">
            <label for="estacionamento"><img src="/vagas.png"></label>

            <input value="1" type="checkbox" style="opacity: 0;" name="ar_condicionado" id="ar_condicionado">
            <label for="ar_condicionado"><img src="/arcondicionado.png"></label>
          </div>
        </div>

      </div>

      </div>
      <div class="container-cont">
        <div class="titulo">
          <p>Valor do Aluguel</p><svg xmlns="http://www.w3.org/2000/svg" width="13" height="27" viewBox="0 0 13 27"
            fill="none">
            <path
              d="M6.46875 1V4.125M6.46875 4.125C11.1562 4.125 11.9375 7.25 11.9375 7.25M6.46875 4.125C1.78125 4.125 1 7.18207 1 8.8125C1 15.2818 11.9375 11.8827 11.9375 18.1875C11.9375 19.8179 11.1562 22.875 6.46875 22.875M6.46875 22.875V26M6.46875 22.875C1.78125 22.875 1 19.75 1 19.75"
